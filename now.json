{
  "name": "url-shortner",
  "version": 2,
  "builds": [{
    "src": "static/**",
    "use": "@now/static"
  }, {
    "src": "main.js",
    "use": "@now/node-server"
  }],
  "routes": [{
      "src": "/",
      "dest": "/static/index.html"
    }, {
      "src": "/404",
      "dest": "/static/404.html"
    }, {
      "src": "/(.+svg|.+png|.+css|.+js|.+ico|robots.txt)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "main.js"
    }
  ]
}
